{
  "name": "grpc-docs",
  "version": "1.1.4",
  "description": "gRPC API Documentation",
  "author": "gendocu-com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gendocu-com/grpc-docs.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs,umd",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^7.2.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^16.9.7",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.18",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^5.0.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.15.0",
    "@types/minimatch": "^3.0.5",
    "GendocuPublicApis": "https://git.gendocu.com/gendocu/GendocuPublicApis.git#master",
    "google-protobuf": "^3.21.2",
    "grpc-docs": "^1.0.6",
    "js-yaml": "^4.1.0",
    "minimatch": "^3.0.4",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^16.13.1",
    "react-is": "^16.8.0",
    "react-syntax-highlighter": "^15.4.5",
    "rollup": "^0.60",
    "rollup-plugin-smart-asset": "^2.1.2",
    "styled-components": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/gendocu-com/grpc-docs/issues"
  },
  "homepage": "https://github.com/gendocu-com/grpc-docs#readme",
